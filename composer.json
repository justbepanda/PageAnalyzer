{
  "name": "hexlet/code",
  "description": "hexlet php project 3",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "justbepanda",
      "email": "justbepanda@gmail.com"
    }
  ],
  "require": {
    "slim/slim": "^4.12",
    "slim/psr7": "^1.6",
    "slim/http": "^1.3",
    "slim/php-view": "^3.2",
    "slim/flash": "^0.4.0",
    "php-di/php-di": "^7.0",
    "symfony/string": "^6.4",
    "illuminate/collections": "^10.48",
    "slim/twig-view": "^3.3",
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.6",
    "twig/twig": "^3.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.8",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/extension-installer": "^1.3",
    "symfony/var-dumper": "^6.4",
    "psy/psysh": "@stable",
    "illuminate/support": "^10.48",
    "mockery/mockery": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Hexlet\\Code\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hexlet\\Code\\Tests\\": "tests/"
    }
  },
  "bin": [
    "public/index.php"
  ]  
}
